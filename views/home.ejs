<%- include("partials/header"); -%>
<h1>Home</h1>
<p> <%=text%> </p>
<% posts.forEach(function(posts){%>
 <h1><%=posts.title%></h1>
 <p>
 	<%=posts.content.substring(0, 100)+"..."%>
 	<a href="/posts/<%=posts._id%>">Read More</a>
 	<input type="hidden" name="postId" value="<%=posts._id%>">
 </p>
<% }) %>
<%- include("partials/footer"); -%>